{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "strict": 0,
        "quotes": 0,
        "curly": 0,
        "no-var": 1,
        "no-alert": 0,
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "no-multi-spaces": 0,
        "no-loop-func": 0,
        "no-console": 0,
        "no-shadow": 0,
        "comma-spacing": 0,
        "eol-last": 0,
    }
}